    "def question_2_factoriel(nbre):\n",
    "    i=nbre\n",
    "    valeur = nbre\n",
    "    while i>1:\n",
    "        i=i-1\n",
    "        valeur=valeur*i\n",
    "    return 'le factoriel de'+ ' ' +str(nbre ) + ':   ' +str(valeur)\n",
    "\n",
    "print(question_2_factoriel(5))\n",
    "#input: 5\n",
    "#output: le factoriel de 5:   120"
   ]
